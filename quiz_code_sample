	<script src="https://code.jquery.com/jquery-1.9.1.js"></script>
	<script type="text/javascript">
		$(document).ready(function () {
			$(".quizcontent").hide();
			$("#Question1").show();

			$(".btn-default").bind("click", function () {
				var btn = $(this);
				btn.addClass("clicked");
				btn.parent().addClass("answered");
				var quizId = btn.data('quiz');
				var quizTitle = btn.data('title');
				var answer = this.value;
				$('#' + quizTitle).find(".btn-default").attr("disabled", "disabled");
				$('#' + quizTitle).find('div.last_btn').removeClass("hide");
				var anstag = $('#' + quizTitle).find('div.answer')
				var divtag = $('#' + quizTitle).find('div.answer-explain');
				var htag = $('<h2/>');
				var ptag = divtag.html();
				divtag.empty();
				if (answer.toLowerCase() == anstag.html().trim().toLowerCase()) {
					htag.addClass("blue").html("Correct");
					btn.addClass("correct")

				}
				else {
					var pcorrect = '<p class="font24px">The correct answer is <span class="blue">' + anstag.html().toUpperCase().trim()  + '.</span></p>';
					ptag = pcorrect + ptag;
					htag.addClass("red").html("Incorrect");
				}
				divtag.append(htag).append(ptag).removeClass("hide");
				$('#' + quizTitle).find('button.nxt').removeAttr("disabled").removeClass("grey-button");
				$('#' + quizTitle).find('button.prev').removeAttr("disabled").removeClass("grey-button");

			});

			$(".nxt").click(function (event) {
				$(".quizcontent").hide();                 
				var nextQuiz = parseInt($(this).data('currentquiz')) + 1;    
				var nextquizdiv = $("#Question" + nextQuiz);
				nextquizdiv.show();
				$('html, body').animate({ scrollTop: nextquizdiv.offset().top - (nextquizdiv.height()+50) }, 800);

			});
			$(".prev").click(function (event) {

				$(".quizcontent").hide();
				var prevQuiz = parseInt($(this).data('currentquiz')) - 1;
				var prevquizdiv = $("#Question" + prevQuiz);

				prevquizdiv.animate({
					scrollTop: prevquizdiv.offset().top + prevquizdiv.height(),
				}, 2000);
				prevquizdiv.show();

			});

		});


</script>
</html>
